{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "entry": [
        "src/app/**/*.{ts,tsx}",
        "scripts/sync/sync.ts",
        "scripts/bundle-report/bundle-report.ts"
    ],
    "project": ["src/**/*.{ts,tsx,js,jsx}", "scripts/**/*.{ts,tsx,js,jsx}"],
    "ignore": ["**/*.config.*"],
    "ignoreDependencies": ["eslint-nibble", "tailwindcss"],
    "ignoreExportsUsedInFile": {
        "interface": true,
        "type": true
    },
    "tags": ["-lintignore"]
}
